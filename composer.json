{
    "name": "kilian/metricmetoy-php",
    "description": "This is a demo project usable as a client lib to write metrics",
    "type": "library",
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "license": "MIT",
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "MetricMetoy\\Logger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MetricMetoy\\Test\\Logger\\": "tests/unit"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
